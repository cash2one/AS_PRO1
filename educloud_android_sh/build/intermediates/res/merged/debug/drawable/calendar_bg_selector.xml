<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
  <item app:state_range_middle="true">
    <color android:color="@color/calendar_selected_range_bg"/>
  </item>
  <item app:state_current_month="false">
    <color android:color="@color/calendar_inactive_month_bg"/>
  </item>
  <item app:state_today="true">
    <color android:color="@color/calendar_selected_day_bg"/>
  </item>
  <item android:state_focused="true" android:drawable="@drawable/kaoqin_choose"/>
  <item android:state_pressed="true" android:drawable="@drawable/kaoqin_choose"/>
  <item android:state_checked="true" android:drawable="@drawable/kaoqin_choose"/>
  <item android:state_selected="true" android:drawable="@drawable/kaoqin_choose"/>
  <item>
    <color android:color="@color/calendar_active_month_bg"/>
  </item>
</selector>
