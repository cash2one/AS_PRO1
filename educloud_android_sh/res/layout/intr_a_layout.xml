<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
	android:background="@drawable/intrpagea">
    
    <ImageView android:id="@+id/ivIntr_a_right_1"
        android:layout_width="176dp"
        android:layout_height="283dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/intrpagea_right_1" />
    
    <ImageView android:id="@+id/ivIntr_a_right_2"
        android:layout_width="241dp"
        android:layout_height="90dp"
        android:layout_marginTop="70dp"
        android:layout_toLeftOf="@+id/ivIntr_a_right_1"
        android:background="@drawable/intrpagea_right_2" />
    
    <ImageView android:id="@+id/ivIntr_a_middle"
        android:layout_width="180dp"
        android:layout_height="110dp"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/ivIntr_a_right_2"
        android:layout_alignParentLeft="true"
        android:background="@drawable/intrpagea_middle" />
    
    <ImageView android:id="@+id/ivIntr_a_left_1"
        android:layout_width="176dp"
        android:layout_height="283dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="@drawable/intrpagea_left_1" />
    
    <ImageView android:id="@+id/ivIntr_a_left_2"
        android:layout_width="241dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="90dp"
        android:layout_toRightOf="@+id/ivIntr_a_left_1"
        android:background="@drawable/intrpagea_left_2" />"

</RelativeLayout>
